  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "concurrently \"npm run server:compile -- -w\" \"npm run server:run\"",
    "start": "npm run server",
    "server:lint": "tslint src/**/*.ts",
    "server:compile": "tsc -p backend",
    "server:run": "nodemon backend/dist --port $PORT",
    "server": "npm run server:compile && npm run server:run",
    "client:build": "ng build --prod --output-path=./dist",
    "docker:build": "npm run docker:build:backend & npm run docker:build:client",
    "docker:build:backend": "./node_modules/typescript/bin/tsc -p backend",
    "docker:build:client": "./node_modules/@angular/cli/bin/ng build --target=production --environment=testing",
    "docker:start": "npm run docker:start:backend & npm run docker:start:client",
    "docker:start:backend": "node ./backend/dist/index.js",
    "docker:start:client": "http-server ./dist/ -p 4200"
  },
  "private": true,
